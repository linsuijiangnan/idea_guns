<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.stylefeng.guns.modular.system.dao.QuestionMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="cn.stylefeng.guns.modular.system.model.Question">
        <id column="question_id" property="questionId" />
        <result column="user_id" property="userId" />
        <result column="question_direction" property="questionDirection" />
        <result column="question_type" property="questionType" />
        <result column="question_picture" property="questionPicture" />
        <result column="question_price" property="questionPrice" />
        <result column="question_photo" property="questionPhoto" />
        <result column="question_describe" property="questionDescribe" />
        <result column="question_responder" property="questionResponder" />
        <result column="question_time" property="questionTime" />
        <result column="question_remake" property="questionRemake" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        user_id, question_id, question_direction, question_type, question_picture, question_price, question_photo, question_describe, question_responder, question_time, question_remake
    </sql>

</mapper>
